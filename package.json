{
  "name": "lean-chat-vscode",
  "displayName": "Lean Chat",
  "description": "Lean to OpenAI Codex chat app.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:lean-chat-vscode.open"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lean-chat-vscode.open",
        "title": "Open chat"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "vscode:prepublish": "webpack --env production",
    "compile": "webpack --env development",
    "watch": "webpack --env development --watch",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.68.0",
    "@vscode/test-electron": "^2.1.3",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "vsce": "^1.91.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "css-loader": "^5.2.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^2.0.0",
    "svg-loader": "0.0.2",
    "ts-loader": "^9.2.2",
    "url-loader": "^4.1.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}